{
  "expo": {
    "scheme": "com.auth.scavengerapp",
    "name": "scavengerApp",
    "slug": "scavenger-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
