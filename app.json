    "android": {
      "package": "com.auth.scavengerapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9f38035c-e9ec-462e-a482-1503ac344892"
      }
    }
  }
